# ðŸ¤– MemeBot Chat - Environment Variables Template
# Copy this to .env.local and add your API keys
# Following 0rug.com coding guidelines

# Supabase Configuration (Required for Authentication)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Telegram Bot Configuration (Required for Auth)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_WEBHOOK_SECRET=your_telegram_webhook_secret_here

# AI API Keys
NEXT_PUBLIC_OPENROUTER_API_KEY=your_openrouter_api_key_here
# NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here (deprecated - using OpenRouter now)

# Solana RPC Configuration
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Optional: Custom RPC endpoints for better performance
# NEXT_PUBLIC_SOLANA_RPC_URL=https://solana-mainnet.rpc.extrnode.com
# NEXT_PUBLIC_SOLANA_RPC_URL=https://rpc.ankr.com/solana

# Cache Configuration (in milliseconds)
NEXT_PUBLIC_CACHE_TTL_PRICE=30000
NEXT_PUBLIC_CACHE_TTL_METADATA=300000
NEXT_PUBLIC_CACHE_TTL_ANALYSIS=600000
NEXT_PUBLIC_CACHE_TTL_AI_RESPONSE=900000

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_ENABLE_LOGGING=false

# API Rate Limiting (requests per minute)
NEXT_PUBLIC_JUPITER_RATE_LIMIT=100
NEXT_PUBLIC_RAYDIUM_RATE_LIMIT=50
NEXT_PUBLIC_SOLANA_RPC_RATE_LIMIT=6000 